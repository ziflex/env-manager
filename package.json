{
  "name": "env-manager",
  "version": "0.2.4",
  "description": "Environment manager for build tasks",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint -c ./.eslintrc.json src",
    "test": "npm run lint && mocha --reporter spec --require babel-register --recursive test",
    "clean": "rimraf lib",
    "build": "npm run test && npm run clean && babel src --out-dir lib"
  },
  "keywords": [
    "build",
    "environment",
    "manager"
  ],
  "engines": {
    "node": ">=0.8.0"
  },
  "author": "Tim Voronov <ziflex@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ziflex/env-manager.git"
  },
  "dependencies": {
    "is": "^3.1.0",
    "lodash.defaultsdeep": "^3.10.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-eslint": "^6.0.3",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "eslint": "^2.8.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-mocha": "^2.2.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  }
}
