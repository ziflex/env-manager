{
  "name": "env-manager",
  "version": "0.3.0",
  "description": "Environment manager for build tasks",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint -c ./.eslintrc.json src",
    "test": "npm run lint && mocha --reporter spec --require @babel/register --recursive test",
    "clean": "rimraf lib",
    "build": "npm run test && npm run clean && babel src --out-dir lib"
  },
  "keywords": [
    "build",
    "environment",
    "manager"
  ],
  "engines": {
    "node": ">=0.8.0"
  },
  "author": "Tim Voronov <ziflex@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ziflex/env-manager.git"
  },
  "dependencies": {
    "is": "^3.3.0",
    "lodash.defaultsdeep": "^4.6.0",
    "lodash.merge": "^4.6.1",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-mocha": "^5.3.0",
    "mocha": "^6.1.3",
    "rimraf": "^2.6.3"
  }
}
